<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pe.com.backus.scaudi.domain.CentroDistribucion" table="CENTRO_DISTRIBUCION">
        <id column="ID_CENTRO_DIST" name="idCentroDist" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        <property length="255" name="descripcion" not-null="true" type="java.lang.String">
            <column name="DESCRIPCION" sql-type="VARCHAR"/>
        </property>
        <many-to-one class="pe.com.backus.scaudi.domain.Region"
            name="region" not-null="true">
            <column name="REGION_ID"/>
        </many-to-one>
    </class>
</hibernate-mapping>

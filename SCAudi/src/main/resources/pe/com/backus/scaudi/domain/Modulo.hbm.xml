<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pe.com.backus.scaudi.domain">
    <class name="Modulo" table="T_MODULO">

        <id name="idModulo" column="IN_IDMODULO">
                <generator class="increment"></generator>
        </id>
        <property name="descripcion" column="VC_DESCRIPCION" length="50"></property>

    </class>
  
</hibernate-mapping>

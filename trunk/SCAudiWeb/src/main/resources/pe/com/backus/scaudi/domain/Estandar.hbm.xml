<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="pe.com.backus.scaudi.domain.Estandar" table="T_ESTANDAR">
    <id column="IN_IDESTANDAR" name="idEstandar" type="java.lang.Integer">
      <generator class="native"/>
    </id>
    <property length="100" name="desripcion" not-null="true" type="java.lang.String">
      <column name="VC_DESCRIPCION" sql-type="VARCHAR"/>
    </property>
    <property name="peso" not-null="false" type="java.lang.Integer">
      <column name="IN_PESO" sql-type="INTEGER"/>
    </property>
    <many-to-one class="pe.com.backus.scaudi.domain.Area" name="area" not-null="true">
      <column name="IN_IDAREA"/>
    </many-to-one>
  </class>
</hibernate-mapping>

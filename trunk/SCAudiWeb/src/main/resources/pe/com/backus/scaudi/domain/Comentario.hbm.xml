<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pe.com.backus.scaudi.domain">
  <class name="Region" table="T_COMENTARIO">
    <id column="IN_IDCOMENTARIO" name="idComentario">
      <generator class="increment"/>
    </id>
    <property column="VC_DESCRIPCION" length="30" name="descripcion"/>
  </class>
</hibernate-mapping>

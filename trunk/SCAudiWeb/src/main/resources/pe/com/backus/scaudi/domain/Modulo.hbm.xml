<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pe.com.backus.scaudi.domain.Modulo" table="MODULO">
        <id column="ID_MODULO" name="idModulo" type="java.lang.Integer">
            <generator class="native"/>
        </id>
        <property length="255" name="descripcion" not-null="true" type="java.lang.String">
            <column name="DESCRIPCION" sql-type="VARCHAR"/>
        </property>
    </class>
</hibernate-mapping>

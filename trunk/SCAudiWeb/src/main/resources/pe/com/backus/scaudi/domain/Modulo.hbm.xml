<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pe.com.backus.scaudi.domain">
  <class name="Modulo" table="T_MODULO">
    <id column="IN_IDMODULO" name="idModulo">
      <generator class="increment"/>
    </id>
    <property column="VC_DESCRIPCION" length="50" name="descripcion"/>
  </class>
</hibernate-mapping>
